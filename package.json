{
  "name": "apiclaude",
  "version": "1.0.0",
  "description": "Hello World API built with Cloudflare Workers",
  "main": "src/index.ts",
  "scripts": {
    "prebuild": "tsx scripts/convert-csv-to-json.ts",
    "build": "wrangler deploy",
    "dev": "npm run prebuild && wrangler dev",
    "deploy": "wrangler deploy",
    "types": "wrangler types",
    "type-check": "npm run types && tsc --noEmit",
