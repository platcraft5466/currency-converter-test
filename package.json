    "test:watch": "vitest"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.71",
    "@cloudflare/workers-types": "^4.20260207.0",
    "@types/node": "^22.10.5",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4",
    "wrangler": "^3.91.0"
  }
}
