{
  // Schema for editor autocomplete and validation
  "$schema": "./node_modules/wrangler/config-schema.json",

  // Worker name (used for workers.dev subdomain and in dashboard)
  // Must be alphanumeric + dashes, max 63 chars for workers.dev
